{
  "name": "redwall",
  "version": "0.0.1",
  "description": "Download and use wallpapers from reddit",
  "author": "Fahad Hossain",
  "homepage": "https://github.com/Deadhood/redwall",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deadhood/redwall.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "run-p dev runDev",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "runDev": "electron . --devtool",
    "build": "webpack --config webpack.build.config.js",
    "runProd": "electron . --noDevServer",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds",
    "lint": "standard"
  },
  "dependencies": {
    "@fa7ad/wallpaper": "^2.4.2-a",
    "electron": "^5.0.6",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "nraw": "^0.2.5",
    "react": "^16.8.6",
    "react-desktop": "^0.3.9",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.11",
    "electron-packager": "^14.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "next",
    "standard": "^13.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^3.7.2"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
